#include <PIDLifterControl.h>
//#include "WPILib.h"


PIDLifterControl::PIDLifterControl(PIDSource *nSource, PIDOutput *nOutput){
	// TODO Auto-generated constructor stub
	this->output = nOutput;
	this->source = nSource;
	p = 0.0;
	i = 0.0;
	d = 0.0;
	controller = new PIDController(p,i,d,output,source);

}

PIDLifterControl::~PIDLifterControl() {
	// TODO Auto-generated destructor stub
}

